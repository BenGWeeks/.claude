{
  "env": {
    "CLAUDE_CODE_DISABLE_TERMINAL_TITLE": "1"
  },
  "model": "opus",
  "statusLine": {
    "type": "command",
    "command": "$HOME/.claude/statusline-command.sh"
  },
  "enabledPlugins": {
    "turbo-whisper-integration@turbo-whisper-marketplace": true,
    "t-minus-15@t-minus-15": false,
    "frontend-design@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true
  },
  "feedbackSurveyState": {
    "lastShownTime": 1753957508481
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash $HOME/.claude/hooks/speak-summary.sh"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "permission_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "bash $HOME/.claude/hooks/speak-notification.sh permission_prompt"
          }
        ]
      },
      {
        "matcher": "elicitation_dialog",
        "hooks": [
          {
            "type": "command",
            "command": "bash $HOME/.claude/hooks/speak-notification.sh elicitation_dialog"
          }
        ]
      },
      {
        "matcher": "idle_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "bash $HOME/.claude/hooks/speak-notification.sh idle_prompt"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "matcher": "auto",
        "hooks": [
          {
            "type": "command",
            "command": "bash $HOME/.claude/hooks/speak-notification.sh pre_compact_auto"
          }
        ]
      },
      {
        "matcher": "manual",
        "hooks": [
          {
            "type": "command",
            "command": "bash $HOME/.claude/hooks/speak-notification.sh pre_compact_manual"
          }
        ]
      }
    ]
  }
}
